   <div class=\"container align-center mbr-white\">\n        <!-- Content -->\n        <div class=\"mbr-row mbr-jc-c flex-al-c\">\n            <!-- Title -->\n            <div class=\"mbr-col-sm-12 mbr-col-md-12 mbr-col-lg-5\" mbr-if=\"showTitle || showSubtitle\">\n                <div class=\"title-wrap\" mbr-if=\"showTitle || showSubtitle\">\n                    <h3 class=\"mbr-white mbr-text mbr-fonts-style mbr-extralight\" mbr-theme-style=\"display-2\" mbr-if=\"showTitle\" mbr-class=\"{'align-left' : titleAlign == 1, 'align-center' : titleAlign == 2, 'align-right' : titleAlign == 3}\"><b>Join Our Subscribe List</b></h3>\n                    <p class=\"mbr-white mbr-fonts-style mbr-bold\" mbr-theme-style=\"display-4\" mbr-if=\"showSubtitle\" mbr-class=\"{'align-left' : titleAlign == 1, 'align-center' : titleAlign == 2, 'align-right' : titleAlign == 3}\">Stay In Touch With Us My Friend!\n                    </p>\n                </div>\n            </div>\n            <!-- Form -->\n            <div mbr-form class=\"mbr-col-sm-12 mbr-col-md-12 mbr-col-lg-7\">\n                <form class=\"mbr-form\" action method=\"post\" name=\"Mobirise Form\" target=\"_top\" novalidate>\n                    <div submit-success>\n                        <template data-form-alert type=\"amp-mustache\">Subscription successful!</template>\n                    </div>\n                    <div submit-error>\n                        <template data-form-alert type=\"amp-mustache\">Subscription failed! {{error}}</template>\n                    </div>\n\n                    <div class=\"fieldset\">\n                        <label class=\"input-label\">\n                            <input type=\"hidden\" name=\"form[data][0][0]\" value=\"Email\">\n                            <input type=\"email\" name=\"form[data][0][1]\" data-form-field=\"Email\" mbr-theme-style=\"display-4\" placeholder=\"Enter your email here ...\" required>\n                            <!-- Button -->\n               