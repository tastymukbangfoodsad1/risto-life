>\n        <input type=\"checkbox\" title=\"Borders:\" name=\"showBorders\" condition=\"showCardsTitle || showCardsText || checkIcons\">\n        <input type=\"color\" title=\"Color Border:\" name=\"bordersColor\" value=\"#cccccc\" condition=\"showBorders && (showCardsTitle || showCardsText || checkIcons)\">\n        <input type=\"checkbox\" title=\"Transparent BG:\" name=\"transparentBg\" checked condition=\"showCardsTitle || showCardsText || checkIcons\">\n        <input type=\"color\" title=\"Color BG:\" name=\"cardBg\" value=\"#efefef\" condition=\"!transparentBg && (showCardsTitle || showCardsText || checkIcons)\">\n        <input type=\"checkbox\" title=\"Icons:\" name=\"checkIcons\" checked>\n        <input type=\"range\" inline title=\"Size icon:\" name=\"iconsSize\" min=\"1\" max=\"8\" step=\"0.5\" value=\"2.5\" condition=\"checkIcons\">\n        <input type=\"color\" title=\"Color Icon:\" name=\"colorIcons\" value=\"#37b721\" condition=\"checkIcons\">\n        <!--  -->\n        <header>Background:</header>\n        <fieldset type=\"background\" name=\"bg\">\n            <input type=\"color\" title=\"Background Color:\" value=\"#f1f1f7\" selected>\n            <input type=\"image\" title=\"Background Image:\" value=\"../_images/background3.jpg\">\n        </fieldset>\n        <input type=\"checkbox\" title=\"Overlay:\" name=\"overlay\" checked condition=\"bg.type !== 'color'\">\n        <input type=\"color\" title=\"Overlay Color:\" name=\"overlayColor\" value=\"#efefef\" condition=\"overlay && bg.type == 'image'\">\n        <input type=\"range\" inline title=\"Opacity:\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.5\" condition=\"overlay && bg.type == 'image'\">\n    </mbr-parameters>\n    <!--  -->\n    <!-- Overalay -->\n    <div class=\"mbr-overlay\" mbr-if=\"overlay && bg.type !== 'color'\"></div>\n    <!-- Container -->\n    <div mbr-class=\"{'container':!fullWidth, 'container-fluid':fullWidth}\">\n        <!-- Title -->\n        <div class=\"title-block mbr-pb-5\" mbr-if=\"showTitle || showSubtitle\" mbr-class=\"{'align-left' : alignTitle == 1, 'align-center' : alignTitle == 2, 'align-right' : alignTitle == 3}\">\n            <h3 class=\"mbr-section-title mbr-fonts-style mbr-pb-2 mbr-bold\" mbr-theme-style=\"display-2\" mbr-if=\"showTitle\">Our Contact</h3>\n            <h4 class=\"mbr-section-subtitle mbr-fonts-style mbr-pb-1 mbr-light\" mbr-theme-style=\"display-7\" mbr-if=\"showSubtitle\">A creative agency for smart businesses.</h4>\n            <div class=\"line\" mbr-if=\"showBorderTitle && (showTitle || showSubtitle)\">\n                <div class=\"line-line-gray\">\n                    <div class=\"line-line-color\"></div>\n                </div>\n            </div>\n        </div>\n        <!-- Content -->\n        <div class=\"mbr-row mbr-px-2 mbr-jc-c mbr-pt-3\" mbr-class=\"{'mbr-column': !flexDirection}\" mbr-if=\"showCardsTitle || showCardsText || checkIcons\">\n        <div class=\"card mbr-col-sm-12\" mbr-class=\"{'mbr-px-4': !flexDirection,\n        'mbr-col-md-12': flexDirection &amp;&amp; cardsCount == 1,\n        'mbr-col-md-6 mbr-col-lg-6': flexDirection &amp;&amp; cardsCount == 2,\n        'mbr-col-md-8 mbr-col-lg-4': flexDirection &amp;&amp; cardsCount == 3,\n        'mbr-col-md-6 mbr-col-lg-3': flexDirection &amp;&amp; cardsCount == 4,\n        'align-center': cardsAlign == 2,\n        'align-right': cardsAlign == 3,\n        'md-pb': (flexDirection &amp;&amp; cardsCount > 2) || (!flexDirection &amp;&amp; cardsCount > 1),\n        'lg-pb': !flexDirection &amp;&amp; cardsCount > 1,\n        'last-child': cardsCount == 1}\">\n        <div class=\"card-wrapper mbr-flex\" mbr-class=\"{'mbr-column': checkIcons && flexDirection,\n        'mbr-pt-3 mbr-px-4 mbr-pb-3': showBorders || !transparentBg}\">\n        <div class=\"card-img\" mbr-if=\"checkIcons\" mbr-class=\"{'align-center': cardsAlign == 2,\n        'align-right': cardsAlign == 3}\">\n        <span mbr-icon class=\"amp-iconfont icon fa-map-marker fa\"></span>\n    </div>\n    <div class=\"card-box mbr-pt-3\" mbr-if=\"(showCardsTitle || showCardsText)\">\n    <h5 class=\"card-title mbr-bold mbr-fonts-style\" mbr-theme-style=\"display-7\" mbr-if=\"showCardsTitle\" data-app-selector=\".card-title\">Address</h5>\n    <p class=\"card-text mbr-fonts-style mbr-pt-2\" mbr-theme-style=\"display-4\" mbr-if=\"showCardsText\" data-app-selector=\".card-text\" mbr-link-type=\"address\">6834 Hollywood Blvd - Los Angeles CA</p>\n</div>\n</div>\n</div>\n<div class=\"card mbr-col-sm-12\" mbr-class=\"{'mbr-px-4': !flexDirection,\n'mbr-col-md-6 mbr-col-lg-6': flexDirection &amp;&amp; cardsCount == 2,\n'mbr-col-md-8 mbr-col-lg-4': flexDirection &amp;&amp; cardsCount == 3,\n'mbr-col-md-6 mbr-col-lg-3': flexDirection &amp;&amp; cardsCount == 4,\n'align-center': cardsAlign == 2,\n'align-right': cardsAlign == 3,\n'md-pb': cardsCount > 2,\n'lg-pb': !flexDirection &amp;&amp; cardsCount > 2,\n'last-child': cardsCount == 2}\" mbr-if=\"cardsCount > 1\">\n<div class=\"card-wrapper mbr-flex\" mbr-class=\"{'mbr-column': checkIcons && flexDirection,\n'mbr-pt-3 mbr-px-4 mbr-pb-3': showBorders || !transparentBg}\">\n<div class=\"card-img\" mbr-if=\"checkIcons\" mbr-class=\"{'align-center': cardsAlign == 2,\n'align-right': cardsAlign == 3}\">\n<span mbr-icon class=\"amp-iconfont icon fa-phone fa\"></span>\n</div>\n<div class=\"card-box mbr-pt-3\" mbr-if=\"(showCardsTitle || showCardsText)\">\n<h5 class=\"card-title mbr-bold mbr-fonts-style\" mbr-theme-style=\"display-7\" mbr-if=\"showCardsTitle\" data-app-selector=\".card-title\">Phone</h5>\n<p class=\"card-text mbr-fonts-style mbr-pt-2\" mbr-theme-style=\"display-4\" mbr-if=\"showCardsText\" data-app-selector=\".card-text\" mbr-link-type=\"phone\">+1234 56 789</p>\n</div>\n</div>\n</div>\n<div class=\"card mbr-col-sm-12\" mbr-if=\"cardsCount > 2\" mbr-class=\"{'mbr-px-4': !flexDirection,\n'mbr-col-md-8 mbr-col-lg-4': flexDirection &amp;&amp; cardsCount == 3,\n'mbr-col-md-6 mbr-col-lg-3': flexDirection &amp;&amp; cardsCount == 4,\n'align-center': cardsAlign == 2,\n'align-right': cardsAlign == 3,\n'md-pb': !flexDirection &amp;&amp; cardsCount > 3,\n'lg-pb': !flexDirection &amp;&amp; cardsCount > 3,\n'last-child': cardsCount == 3}\">\n<div class=\"card-wrapper mbr-flex\" mbr-class=\"{'mbr-column': checkIcons && flexDirection,\n'mbr-pt-3 mbr-px-4 mbr-pb-3': showBorders || !transparentBg}\">\n<div class=\"card-img\" mbr-if=\"checkIcons\" mbr-class=\"{'align-center': cardsAlign == 2,\n'align-right': cardsAlign == 3}\">\n<span mbr-icon class=\"amp-iconfont icon fa-envelope-o fa\"></span>\n</div>\n<div class=\"card-box mbr-pt-3\" mbr-if=\"(showCardsTitle || showCardsText)\">\n<h5 class=\"card-title mbr-bold mbr-fonts-style\" mbr-theme-style=\"display-7\" mbr-if=\"showCardsTitle\" data-app-selector=\".card-title\">E-mail</h5>\n<p class=\"card-text mbr-pt-3 mbr-fonts-style\" mbr-theme-style=\"display-4\" mbr-if=\"showCardsText\" data-app-selector=\".card-text\" mbr-link-type=\"email\">testmail@demo.com</p>\n</div>\n</div>\n</div>\n<div class=\"card mbr-col-sm-12\" mbr-class=\"{'mbr-col-md-6 mbr-col-lg-3': flexDirection,\n'mbr-px-4': !flexDirection,\n'align-center': cardsAlign == 2,\n'align-right': cardsAlign == 3,\n'last-child': cardsCount == 4}\" mbr-if=\"cardsCount > 3\">\n<div class=\"card-wrapper mbr-flex\" mbr-class=\"{'mbr-column': checkIcons && flexDirection,\n'mbr-pt-3 mbr-px-4 mbr-pb-3': showBorders || !transparentBg}\">\n<div class=\"card-img\" mbr-if=\"checkIcons\" mbr-class=\"{'align-center': cardsAlign == 2,\n'align-right': cardsAlign == 3}\">\n<span mbr-icon class=\"amp-iconfont icon fa-envelope-o fa\"></span>\n</div>\n<div class=\"card-box mbr-pt-3\" mbr-if=\"(showCardsTitle || showCardsText)\">\n<h5 class=\"card-title mbr-bold mbr-fonts-style\" mbr-theme-style=\"display-7\" mbr-if=\"showCardsTitle\" data-app-selector=\".card-title\">E-mail</h5>\n<p class=\"card-text mbr-fonts-style mbr-pt-3\" mbr-theme-style=\"di